{
    "compilerOptions": {
      "target": "ES2015",
      "downlevelIteration": true,
      "lib": ["dom", "es2015"],
      "jsx": "react",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true
    },
    "include": ["src/**/*", "httpolyglot.d.ts"],
    "exclude": ["node_modules", "public"]
  }